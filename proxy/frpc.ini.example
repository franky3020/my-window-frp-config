[common]
server_addr = <ip>
server_port = 8002
protocol = websocket
token = <token>

[Windows-remote-client]
type = udp
role = visitor
# 以下 server_name 對應 frpc-proxy.ini 的中括號所框住的name
server_name = Windows-remote
bind_addr = 127.0.0.1
bind_port = 7777
use_encryption = true
use_compression = true


[Windows-remote-client-2]
type = tcp
role = visitor
# 以下 server_name 對應 frpc-proxy.ini 的中括號所框住的name
server_name = Windows-remote-2
bind_addr = 127.0.0.1
bind_port = 7777
use_encryption = true
use_compression = true



